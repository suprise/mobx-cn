{
  "name": "mobx-cn",
  "description": "MobX 中文文档",
  "scripts": {
    "clean": "rimraf _book",
    "build": "gitbook build -g suprise/mobx-cn",
    "watch": "gitbook serve",
    "publish": "npm run clean && npm run build && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:suprise/mobx-cn gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suprise/mobx-cn.git"
  },
  "keywords": [
    "mobx",
    "observable"
  ],
  "author": "Sulpres <yueyuan.cx@alibaba-inc.com> (https://github.com/suprise)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suprise/mobx-cn/issues"
  },
  "homepage": "https://github.com/suprise/mobx-cn",
  "devDependencies": {
    "ebook-convert": "^1.0.0",
    "gitbook-cli": "^0.3.4",
    "gitbook-plugin-advanced-emoji": "^0.2.1",
    "gitbook-plugin-edit-link": "^1.4.1",
    "gitbook-plugin-ga": "^0.2.1",
    "gitbook-plugin-prism": "^0.1.1",
    "rimraf": "^2.3.4",
    "svgexport": "^0.3.0"
  }
}
